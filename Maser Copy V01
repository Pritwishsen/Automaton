			Thread.sleep(5000);
			boolean result1=KeywordsCoreFunctions.isdisplayed(By.xpath("//table[@class='mGrid']"));
			if(result1==true){
			System.out.println("Able to Open the case");
			return true;
			}
			else{
				
				return false;
			}
			
		}else{
			System.out.println("UnAble to Open the case");
			return false;
			
		}
			
	}catch (Exception e) {
