	  KeywordsCoreFunctions.click(By.xpath("//div/a[text()='SCF Complete - Store Contract']"));
	  Thread.sleep(10000);
	  KeywordsCoreFunctions.click(By.xpath("//div/a[text()='Submit']"));
	 }
	 ContractStoretoRepositoryUK((By) elem, comb2); 

}
}
catch(Exception e) 
{
e.printStackTrace();
return false;
}
return true;

