						wait.until(ExpectedConditions.visibilityOfElementLocated(By.xpath("//li[contains(text(),'"+value+"')]")));
						KeywordsCoreFunctions.click(By.xpath("//li[contains(text(),'"+value+"')]"));
						break;
					
