						 JavascriptExecutor js = (JavascriptExecutor)driver;
					switch (charc) {
					
					case "ConsumerName":
						 
						WebElement Name1 = driver.findElement(By.xpath("//span[contains(text(),'Name:')]/following::input[@type='text'][1]"));
				        
