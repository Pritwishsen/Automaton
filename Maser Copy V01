					 Uploadfile((By) elem, filepath);
					 Thread.sleep(10000);
//					 ValidateContractForm((By) elem, comb1);
					 KeywordsCoreFunctions.click(By.xpath("//input[@type='submit']"));
					 Thread.sleep(10000);
