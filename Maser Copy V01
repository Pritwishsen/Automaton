					 {
					 case "Group Enterprise Legal Request": ContractSupportNew((By) elem, ReqForm);
					                                        break;
					 
					 case "UK Enterprise Legal Request": ContractSupportNewUK((By) elem, ReqForm);
                                                            break;
					 default:System.out.println("In default case");
			                  break;
					 }
					 
